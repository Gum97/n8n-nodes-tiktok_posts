{
  "name": "tiktok-n8n-posts",
  "version": "1.0.0",
  "description": "N8N node for TikTok integration - Automated posting and content management",
  "keywords": [
    "n8n-community-node-package",
    "tiktok",
    "social media",
    "automation",
    "content management"
  ],
  "license": "MIT",
  "homepage": "https://github.com/C0d3Sn4p/tiktok_n8n_posts",
  "author": {
    "name": "Nilson Arcuri",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/C0d3Sn4p/tiktok_n8n_posts.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TikTokApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TikTok/TikTok.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}
